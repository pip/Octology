REM Attempt to port GetPrimes from my HP48GX into 4DOS batch
REM         I now think my previous attempt was a great success
set nm=1
set pf=c:\NewPrime.txt
set pp=c:\Primes.txt
inkey /c /w0 %%ky
set sz=%1
if "%sz" == "" set sz=16
echo No point to filling the file if I can't write the middle
goto nofill
echos Filling file %pf up to %sz ÿ
timer on /3
echo Y > %pf
:filloopie
set nm=%@inc[%nm]
echo Y >> %pf
if %nm LT %sz goto filloopie
echos Finished filling... ÿ
timer /3
:nofill
set ph=%@fileopen[%pf,read,t]
set ln=1
set pt=%@fileseekl[%ph,%ln]
:nextprime
echo.
    set ln=%@inc[%ln]
echos ln%ln killing:
    set pt=%@fileread[%ph]
    if "%pt" == "" .and. %@eval[%ln*%ln] LT %sz goto nextprime
        set nm=%ln
        set nm=%@eval[%nm+%ln]
:killthem
            inkey /p /w0 %%ky
            if "%ky" == "" goto end
            set pt=%@fileseekl[%ph,%nm]
            set pt=%@filewrite[%ph,N]
echos %nmÿ
            set nm=%@eval[%nm+%ln]
            if %nm LE %sz goto killthem
            set pt=%@fileseekl[%ph,%ln]
            if %@eval[%ln*%ln] LT %sz goto nextprime
:end
        set pt=%@fileclose[%ph]
echo.
echo what now?
set pt=
set nm=
set ln=
set pf=
set ph=
set sz=


