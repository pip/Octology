#!/usr/bin/perl -w
# 43SBPRK - choo created by Pip Stuart <Pip@CPAN.Org>
#   to calculate n choose m
use strict; use Math::BaseCnv qw(:all);
my $ennn = shift; my $emmm = shift;
die "USAGE: `choo n m` to calculate  n choose m\n" unless(defined($ennn) &&
                                                          defined($emmm));
print choo($ennn, $emmm);
