#!/usr/bin/perl
use strict;use warnings;use utf8;use v5.10;use Octology::U8;
open my $out8,'>&',STDOUT or die "Can't open  duplic8 STDOUT handle: $!";binmode $out8,':encoding(UTF-8)';print $out8 Upd8(@ARGV); # crE8 local duplic8
close   $out8             or die "Can't close duplic8 STDOUT handle: $!";
